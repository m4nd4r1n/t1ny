{
  "name": "t1ny",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node make-cert.js && cross-env NEXT_PUBLIC_HTTP_SCHEME=https next dev --experimental-https --experimental-https-key certificates/key.pem --experimental-https-cert certificates/cert.pem --experimental-https-ca \"$(mkcert -CAROOT)\"/rootCA.pem",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest -u",
    "format": "prettier --write --cache \"**/*.{ts,tsx,md,css}\"",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:reset": "supabase db reset",
    "db:gen-types": "supabase gen types typescript --local > src/types/db.ts && prettier --write --cache src/types/db.ts",
    "email:dev": "email dev -p 4000",
    "email:export": "email export --outDir supabase/templates --pretty"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
